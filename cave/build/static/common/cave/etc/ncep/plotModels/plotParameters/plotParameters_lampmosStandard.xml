<?xml version="1.0" encoding="UTF-8"?>
<PlotParameters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xsi:noNamespaceSchemaLocation="plotParameters.xsd">
<!-- some derived data not in -->  
  
    <!-- TEMPERATURE --> 
     
	<Parameter  name="TMPF" plotMode="text" plotParam="temperature" plotUnit="째F" plotFormat="%3.0f"/>
	<Parameter  name="TMPC" plotMode="text" plotParam="temperature" plotUnit="째C" plotFormat="%3.0f"/>
	<Parameter  name="DWPF" plotMode="text" plotParam="dewpoint" plotUnit="째F" plotFormat="%3.0f"/>
	<Parameter  name="DWPC" plotMode="text" plotParam="dewpoint" plotUnit="째C" plotFormat="%3.0f"/>
	
	<!-- STATION VARIABLES -->
    <Parameter  name="STID" plotMode="text" plotParam="stationId"/>
    
    <!-- CLOUD -->
	<Parameter  name="SKYC" plotMode="table" class="special" plotParam="clouds_bestCat" plotLookupTable="mos_clouds_gfs_s2s.txt" />   <!-- plotFunctionTable="cloud_select.txt" --> 	
	
	<!-- WIND -->
	<Parameter  name="BRBK" plotMode="barb" class="barb" plotParam="windSpeedInflated,windDir" plotLookupTable="wx_symbol_trans.txt"  transform="rotate(0,0,0)"/>
	<Parameter  name="GUST" plotMode="text" plotParam="stdMaxWindSpeed" plotUnit="kts" plotFormat="%3.0f"/>     
	    
	<!-- PRECIPITATION -->
	
	<Parameter  name="PP06" plotMode="text" plotParam="PQPF_6hr" plotUnit="%" plotFormat="%3.0f"/>
	<Parameter  name="PP12" plotMode="text" plotParam="PQPF_12hr" plotUnit="%" plotFormat="%3.0f"/>
	<Parameter  name="PP24" plotMode="text" plotParam="PQPF_24hr" plotUnit="%" plotFormat="%3.0f"/>
	    
	<!-- WEATHER -->    
    <Parameter  name="WSYM" plotMode="table" class="weather" plotParam="presWeather"  transform="rotate(0,0,0)" plotLookupTable="wx_symbol_trans.txt"/>
    
    <!-- MISCELLANEOUS -->


</PlotParameters>
