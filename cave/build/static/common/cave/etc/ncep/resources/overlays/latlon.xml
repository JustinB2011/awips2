<bundle>
  <displayList>
    <displays xsi:type="NC-MapRenderableDisplay"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <descriptor xsi:type="mapDescriptor">
        <resource>
          <properties renderingOrderId="MAP_OUTLINE"></properties>
          <resourceData xsi:type="NC-LatLonOverlayResourceData">
            <legendColor>${legend_color}</legendColor>
            <color>${color}</color>
            <lineWidth>${lineWidth}</lineWidth>
            <lineStyle>${lineStyle}</lineStyle>
            <latitudeInterval>${latitudeInterval}</latitudeInterval>
            <longitudeInterval>${longitudeInterval}</longitudeInterval>
          </resourceData>
        </resource>
      </descriptor>
    </displays>
  </displayList>
</bundle>
