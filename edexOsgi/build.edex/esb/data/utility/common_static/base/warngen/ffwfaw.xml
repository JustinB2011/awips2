  <duration>540</duration>
  <duration>600</duration>
  <duration>660</duration>
  <duration>720</duration>
  <duration>1440</duration>
  <duration>2160</duration>
  <duration>2880</duration>
  <duration>3600</duration>
 </durations>
 
