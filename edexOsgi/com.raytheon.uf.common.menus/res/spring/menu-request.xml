<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:amq="http://activemq.apache.org/schema/core"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd   
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

  <bean factory-bean="menuCreationRegistry" class="com.raytheon.uf.common.menus.MenuCreationRegistry"/>
  <bean id="menuCreationHandler" class="com.raytheon.uf.common.menus.MenuCreationHandler"/>
  
  <bean factory-bean="handlerRegistry" factory-method="register">
  	  <constructor-arg value="com.raytheon.uf.common.menus.MenuCreationRequest"/>
  	  <constructor-arg ref="menuCreationHandler"/>
  </bean>
</beans>