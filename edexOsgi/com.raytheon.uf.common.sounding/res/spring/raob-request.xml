<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:amq="http://activemq.apache.org/schema/core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd   
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
  	
  	<bean id="raobMenuUtil" class="com.raytheon.uf.common.sounding.util.RaobMenuUtil" />
  	<bean id="menuCreationRegistry" class="com.raytheon.uf.common.menus.MenuCreationRegistry" factory-method="getInstance" />	
  	<bean id="raobMenus" factory-bean="menuCreationRegistry" factory-method="register">
  		<constructor-arg value="raob"/>
  		<constructor-arg ref="raobMenuUtil"/>
  	</bean>
</beans>