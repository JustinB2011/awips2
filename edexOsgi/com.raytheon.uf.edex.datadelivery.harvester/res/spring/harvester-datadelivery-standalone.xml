<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

    <!-- Empty bean that is used to depend on registry initialization. Any 
        beans that must wait for the registry to initialize should wait on this bean. 
        In this mode the datadelivery JVM is running separate from the registry, 
        and no initialization is required. -->
    <bean id="registryInit" class="java.lang.String" />

</beans>