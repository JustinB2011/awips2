<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:amq="http://activemq.apache.org/schema/core"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd   
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="bufrsgwhPluginName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="bufrsgwh" />
	</bean>

	<bean id="bufrsgwhProperties" class="com.raytheon.uf.common.dataplugin.PluginProperties">
		<property name="pluginName" ref="bufrsgwhPluginName" />
		<property name="pluginFQN" value="gov.noaa.nws.ncep.common.dataplugin.bufrsgwh" />
		<property name="dao" value="gov.noaa.nws.ncep.common.dataplugin.bufrsgwh.dao.BufrsgwhDao" />
		<property name="record"
			value="gov.noaa.nws.ncep.common.dataplugin.bufrsgwh.BufrSgwhRecord" />
	</bean>
	
    <bean id="bufrsgwhRegistered" factory-bean="pluginRegistry" factory-method="register">
		<constructor-arg ref="bufrsgwhPluginName" />
		<constructor-arg ref="bufrsgwhProperties" />
	</bean>

</beans>