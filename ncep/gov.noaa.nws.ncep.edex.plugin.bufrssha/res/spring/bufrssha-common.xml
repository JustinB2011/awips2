<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:amq="http://activemq.apache.org/schema/core"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd   
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="bufrsshaPluginName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="bufrssha" />
	</bean>

	<bean id="bufrsshaProperties" class="com.raytheon.uf.common.dataplugin.PluginProperties">
		<property name="pluginName" ref="bufrsshaPluginName" />
		<property name="pluginFQN" value="gov.noaa.nws.ncep.common.dataplugin.bufrssha" />
		<property name="dao" value="gov.noaa.nws.ncep.common.dataplugin.bufrssha.dao.BufrsshaDao" />
		<property name="record"
			value="gov.noaa.nws.ncep.common.dataplugin.bufrssha.BufrSshaRecord" />
	</bean>
	
    <bean id="bufrsshaRegistered" factory-bean="pluginRegistry" factory-method="register">
		<constructor-arg ref="bufrsshaPluginName" />
		<constructor-arg ref="bufrsshaProperties" />
	</bean>

</beans>