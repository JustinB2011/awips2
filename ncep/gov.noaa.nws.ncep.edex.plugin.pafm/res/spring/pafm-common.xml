<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:amq="http://activemq.apache.org/schema/core"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd   
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">


	<bean id="pafmPluginName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="pafm" />
	</bean>

	<bean id="pafmProperties" class="com.raytheon.uf.common.dataplugin.PluginProperties">
		<property name="pluginName" ref="pafmPluginName" />
		<property name="pluginFQN" value="gov.noaa.nws.ncep.common.dataplugin.pafm" />
		<property name="dao" value="gov.noaa.nws.ncep.common.dataplugin.pafm.dao.PafmDao" />
        <property name="record" value="gov.noaa.nws.ncep.common.dataplugin.pafm.PafmRecord" />
	</bean>

	<bean factory-bean="pluginRegistry" factory-method="register">
		<constructor-arg ref="pafmPluginName"/>
		<constructor-arg ref="pafmProperties"/>
	</bean>
    
</beans>