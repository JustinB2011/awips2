                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="patternMatchStyle" use="required">
            <annotation>
               <documentation>
                  Only four match styles are supported. They are: exact, prefix, postfix and contain
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
            <simpleType>
               <restriction base="string">
                  <enumeration value="exact">
                  </enumeration>
                  <enumeration value="prefix">
                  </enumeration>
                  <enumeration value="postfix">
                  </enumeration>
                  <enumeration value="contain">
                  </enumeration>
               </restriction>
            </simpleType>
         </attribute>
