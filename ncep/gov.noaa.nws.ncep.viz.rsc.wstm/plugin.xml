<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.raytheon.uf.viz.core.resource">
      <resource
            class="gov.noaa.nws.ncep.viz.rsc.wstm.rsc.WstmResource"
            name="WSTM"
            recordClass="gov.noaa.nws.ncep.common.dataplugin.aww.AwwRecord"
            renderingOrderId="PLOT"
            resourceType="PLAN_VIEW">
      </resource>
   </extension>
    <extension
         point="com.raytheon.uf.viz.core.scriptTemplate">
      <scriptTemplate
            pluginName="wstm"
            scriptLibrary="BaseRequest">
      </scriptTemplate>
   </extension>
   <extension
         point="gov.noaa.nws.ncep.viz.resources.NC-Resource">
       <nc-resource
            class="gov.noaa.nws.ncep.viz.rsc.wstm.rsc.WstmResourceData"
            editDialogClass="gov.noaa.nws.ncep.viz.rsc.wstm.rsc.EditWstmAttrDialog"
            name="WSTM">
      </nc-resource>
   </extension> 
         <extension
         point="gov.noaa.nws.ncep.viz.resources.NC-ResourceAttribute">
      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="wstmWarningEnable"
            defaultValue="true"
            ncResourceName="WSTM">
      </nc-resourceAttribute>
      <nc-resourceAttribute
            attributeClass="org.eclipse.swt.graphics.RGB"
            attributeName="wstmWarningColor"
            defaultValue="RGB {255,0,0}"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
      <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmWarningLineWidth"
            defaultValue="3"
            ncResourceName="WSTM">
       </nc-resourceAttribute>
       <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmWarningSymbolWidth"
            defaultValue="3"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
       <nc-resourceAttribute
            attributeClass="java.lang.Float"
            attributeName="wstmWarningSymbolSize"
            defaultValue="1.0"
            ncResourceName="WSTM">
      </nc-resourceAttribute>

      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="wstmWatchEnable"
            defaultValue="true"
            ncResourceName="WSTM">
      </nc-resourceAttribute>
      <nc-resourceAttribute
            attributeClass="org.eclipse.swt.graphics.RGB"
            attributeName="wstmWatchColor"
            defaultValue="RGB {255,127,0}"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
      <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmWatchLineWidth"
            defaultValue="3"
            ncResourceName="WSTM">
       </nc-resourceAttribute>
       <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmWatchSymbolWidth"
            defaultValue="3"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
       <nc-resourceAttribute
            attributeClass="java.lang.Float"
            attributeName="wstmWatchSymbolSize"
            defaultValue="1.0"
            ncResourceName="WSTM">
      </nc-resourceAttribute>
      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="wstmAdvisoryEnable"
            defaultValue="true"
            ncResourceName="WSTM">
      </nc-resourceAttribute>
      <nc-resourceAttribute
            attributeClass="org.eclipse.swt.graphics.RGB"
            attributeName="wstmAdvisoryColor"
            defaultValue="RGB {255,255,0}"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
      <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmAdvisoryLineWidth"
            defaultValue="3"
            ncResourceName="WSTM">
       </nc-resourceAttribute>
       <nc-resourceAttribute
            attributeClass="java.lang.Integer"
            attributeName="wstmAdvisorySymbolWidth"
            defaultValue="3"
            ncResourceName="WSTM">
      </nc-resourceAttribute>         
       <nc-resourceAttribute
            attributeClass="java.lang.Float"
            attributeName="wstmAdvisorySymbolSize"
            defaultValue="1.0"
            ncResourceName="WSTM">
      </nc-resourceAttribute>
      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="timeEnable"
            defaultValue="true"
            ncResourceName="WSTM">
      </nc-resourceAttribute> 
      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="zoneNameEnable"
            defaultValue="true"
            ncResourceName="WSTM">
      </nc-resourceAttribute>  
      <nc-resourceAttribute
            attributeClass="java.lang.Boolean"
            attributeName="outlineEnable"
            defaultValue="false"
             ncResourceName="WSTM">
      </nc-resourceAttribute>             
     </extension> 
</plugin>
