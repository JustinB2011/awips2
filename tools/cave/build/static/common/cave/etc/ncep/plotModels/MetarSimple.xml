<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<PlotModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="plotModel.xsd"
        name="simple"
        category="METAR"
        svgTemplate="standardPlotModel.svg">

        <PlotModelElement textStyle="Normal" textFont="Helvetica" textSize="12" paramName="TMPF">
            <Position>UL</Position>
            <Color red="255" green="0" blue="0"/>
        </PlotModelElement>
        <PlotModelElement symbolSize="1" paramName="WSYM">
            <Position>ML</Position>
            <Color red="255" green="255" blue="255"/>
        </PlotModelElement>
        <PlotModelElement textStyle="Normal" textFont="Helvetica" textSize="12" paramName="DWPF">
            <Position>LL</Position>
            <Color red="0" green="255" blue="0"/>
        </PlotModelElement>
        <PlotModelElement symbolSize="1" paramName="BRBK">
            <Position>MC</Position>
            <Color red="255" green="255" blue="255"/>
        </PlotModelElement>
</PlotModel>
