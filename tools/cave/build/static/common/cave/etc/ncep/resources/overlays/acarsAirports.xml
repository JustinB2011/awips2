<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bundle>
    <displayList>
        <displays xsi:type="NC-MapRenderableDisplay" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <descriptor xsi:type="mapDescriptor">
                <resource>
                    <properties isSystemResource="false" isBlinking="false" isMapLayer="true" isHoverOn="false" isVisible="true">
                        <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
                    </properties>
                    <resourceData xsi:type="NC-SPIResourceData">
                        <filename>airports.spi</filename>
                        <mapName>ACARS Airport Locations</mapName>
                                <legendColor>${legend_color}</legendColor>
			        <color>${color}</color>
            			<markerState>${markerState}</markerState>
            			<markerType>${markerType}</markerType>
            			<markerSize>${markerSize}</markerSize>
            			<markerWidth>${markerWidth}</markerWidth>
            			<markerTextSize>${markerTextSize}</markerTextSize>
                    </resourceData>
                </resource>
            </descriptor>
        </displays>
    </displayList>
</bundle>
