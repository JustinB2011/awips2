<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bundle>
  <displayList>
    <displays xsi:type="NC-MapRenderableDisplay"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <descriptor xsi:type="mapDescriptor">
        <resource>
          <loadProperties>
            <resourceType>PLAN_VIEW</resourceType>
          </loadProperties>
          <properties isSystemResource="false" isBlinking="false"
            isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true" />
          <resourceData xsi:type="NC-OverlayResourceGroupData">
            <resource>
              <loadProperties>
                <resourceType>PLAN_VIEW</resourceType>
              </loadProperties>
              <properties isSystemResource="false" isBlinking="false"
                isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true">
                <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
              </properties>
              <resourceData xsi:type="NC-dbOverlayResourceData">
                <table>mapdata.world</table>
                <labelField>name</labelField>
                <mapName>World</mapName>
                <constraint>name not in ('CANADA', 'MEXICO', 'UNITED STATES, THE')</constraint>
              </resourceData>
            </resource>
            <resource>
              <loadProperties>
                <resourceType>PLAN_VIEW</resourceType>
              </loadProperties>
              <properties isSystemResource="false" isBlinking="false"
                isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true">
                <pdProps maxDisplayWidth="100000000" minDisplayWidth="750000" />
              </properties>
              <resourceData xsi:type="NC-dbOverlayResourceData">
                <table>mapdata.states</table>
                <labelField>name</labelField>
                <mapName>State Boundaries</mapName>
              </resourceData>
            </resource>
            <resource>
              <loadProperties>
                <resourceType>PLAN_VIEW</resourceType>
              </loadProperties>
              <properties isSystemResource="false" isBlinking="false"
                isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true">
                <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
              </properties>
              <resourceData xsi:type="NC-dbOverlayResourceData">
                <table>mapdata.canada</table>
                <labelField>name</labelField>
                <mapName>Canada</mapName>
              </resourceData>
            </resource>
            <resource>
              <loadProperties>
                <resourceType>PLAN_VIEW</resourceType>
              </loadProperties>
              <properties isSystemResource="false" isBlinking="false"
                isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true">
                <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
              </properties>
              <resourceData xsi:type="NC-dbOverlayResourceData">
                <table>mapdata.mexico</table>
                <labelField>name</labelField>
                <mapName>Mexico</mapName>
              </resourceData>
            </resource>
            <resource>
              <loadProperties>
                <resourceType>PLAN_VIEW</resourceType>
              </loadProperties>
              <properties isSystemResource="false" isBlinking="false"
                isMapLayer="true" isHoverOn="false" opacity="1.0" isVisible="true">
                <pdProps maxDisplayWidth="750000" minDisplayWidth="0" />
              </properties>
              <resourceData xsi:type="NC-dbOverlayResourceData">
                <table>mapdata.county</table>
                <labelField>countyname</labelField>
                <mapName>County Boundaries</mapName>
              </resourceData>
            </resource>
            <mapName>Geo-Political</mapName>
            <lineWidth>${lineWidth}</lineWidth>
            <lineStyle>${lineStyle}</lineStyle>
            <legendColor>${legend_color}</legendColor>
            <color>${color}</color>
          </resourceData>
        </resource>
      </descriptor>
    </displays>
  </displayList>
</bundle>
