<bundle>
  <displayList>
    <displays xsi:type="NC-MapRenderableDisplay"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <descriptor xsi:type="mapDescriptor">
        <resource>
          <properties renderingOrderId="MAP_OUTLINE"></properties>
          <resourceData xsi:type="NC-ScaleOverlayResourceData">
            <legendColor>${legend_color}</legendColor>
            <color>${color}</color>
            <scalePosition>${scalePosition}</scalePosition>
            <scaleUnit>${scaleUnit}</scaleUnit>
            <scaleIntervalMode>${scaleIntervalMode}</scaleIntervalMode>
            <scaleIntervalValue>${scaleIntervalValue}</scaleIntervalValue>
            <scaleLatMode>${scaleLatMode}</scaleLatMode>
            <scaleEffectiveLatitudeValue>${scaleEffectiveLatitudeValue}</scaleEffectiveLatitudeValue>
            <scaleTextFont>${scaleTextFont}</scaleTextFont>
            <scaleTextSize>${scaleTextSize}</scaleTextSize>
            <scaleTextStyle>${scaleTextStyle}</scaleTextStyle>
          </resourceData>
        </resource>
      </descriptor>
    </displays>
  </displayList>
</bundle>
