   <windowImages/>

   <launcher name="alertviz">
      <solaris/>
      <win useIco="false">
         <bmp/>
      </win>
   </launcher>

   <vm>
      <linux include="true">jdk1.6.0</linux>
      <windows include="true">jdk1.6.0</windows>
   </vm>

   <plugins>
   </plugins>

